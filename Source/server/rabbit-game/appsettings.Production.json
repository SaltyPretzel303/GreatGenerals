{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:9091"
      }
    }
  },
  "RabbitConfig": {
    "HostName": "gg-broker",
    "Port": 5672,
    "UserName": "gg_user",
    "Password": "gg_password",
    "VHost": "gg_host"
  },
  "QueuesConfig": {
    "MatchAllWildcard": "#",
    "RoomsRequestTopic": "gg_rooms_req",
    "RoomsResponseTopic": "gg_rooms_res",
    "NewRoomRoute": "new_room.",
    "JoinRoomRoute": "join_room.",
    "LeaveRoomRoute": "leave_room.",
    "StartGameRoute": "start_game.",
    "RoomResponseRoute": "room_response.",
    "RoomUpdateRoute": "room_update.",
    "ModelEventTopic": "gg_model_event",
    "ModelEventRoute": "model_event.",
    "ServerMessageTopic": "gg_server_message",
    "ServerMessageRoutePrefix": "message."
  },
  "LoginServer": {
    "Address": "gg-login-server",
    "Port": 9090,
    "GetPlayerPath": "getplayer",
    "NameArgument": "name"
  },
  "MongoConfig": {
    "MongoUrl": "gg-game-db",
    "MongoUser": "gg_user",
    "MongoPassword": "gg_password",
    "DatabaseName": "gg_games",
    "GamesCollection": "game_info"
  },
  "GameConfig": {
    "Colors": [
      "#ff0000",
      "#00ff00",
      "#0000ff",
      "#aa0044",
      "#00aa44"
    ],
    "DefaultPoints": 100,
    "DefaultMapSizeX": 20,
    "DefaultMapSizeY": 20,
    "DefaultPositions": [
      {
        "x": 3,
        "y": 1
      },
      {
        "x": 10,
        "y": 10
      },
      {
        "x": 4,
        "y": 5
      },
      {
        "x": 5,
        "y": 5
      },
      {
        "x": 5,
        "y": 10
      },
      {
        "x": 4,
        "y": 7
      }
    ],
    "MinimumPlayers": 2,
    "MaximumPlayers": 5,
    "Moves": [
      {
        "type": "BasicWalk",
        "range": 1,
        "speed": 2500,
        "terrainMultiplier": 1
      },
      {
        "type": "BasicFly",
        "range": 2,
        "speed": 1000,
        "terrainMultiplier": 0.5
      }
    ],
    "Units": [
      {
        "unitName": "basicunit",
        "moveType": "BasicWalk",
        "attacks": [
          "BasicAttack"
        ]
      }
    ],
    "Attacks": [
      {
        "type": "BasicAttack",
        "damage": 10,
        "range": 1,
        "duration": 1500,
        "cooldown": 3000
      }
    ]
  }
}